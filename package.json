{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=2048 nuxt generate",
    "build:vercel": "cross-env NODE_OPTIONS=--max-old-space-size=2048 nuxt generate",
    "build:vercel-simple": "cross-env NODE_OPTIONS=--max-old-space-size=2048 nuxt generate",
    "build:vercel-copy": "cross-env NODE_OPTIONS=--max-old-space-size=2048 nuxt generate && cp -r .output .vercel/output",
    "prepare:vercel": "powershell -ExecutionPolicy Bypass -File scripts/prepare-vercel.ps1",
    "deploy": "powershell -ExecutionPolicy Bypass -File scripts/deploy.ps1",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "build:analyze": "cross-env ANALYZE=true nuxt build"
  },
  "dependencies": {
    "@nuxt/eslint": "^1.8.0",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/icon": "^1.15.0",
    "@nuxt/image": "^1.11.0",
    "@nuxtjs/i18n": "latest",
    "@pinia/nuxt": "^0.11.2",
    "eslint": "^9.32.0",
    "nuxt": "^4.0.1",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.4.1",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@iconify-json/fluent": "^1.2.28",
    "@iconify-json/fluent-emoji": "^1.2.3",
    "@iconify-json/heroicons": "^1.2.2",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "cross-env": "^7.0.3"
  }
}
