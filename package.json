{
  "name": "serpenter",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.11.1"
  },
  "scripts": {
    "build": "nuxt build",
    "build:versioned": "node scripts/build-with-version.js",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "build:netlify": "NODE_OPTIONS=--max-old-space-size=8192 npx nuxi build --preset=netlify",
    "build:netlify:spa": "node scripts/build-netlify-spa.js",
    "deploy:netlify": "netlify deploy --prod --dir=.netlify/publish --functions=.netlify/functions",
    "publish:netlify": "npm run build:netlify && npm run deploy:netlify",
    "publish:netlify:spa": "npm run build:netlify:spa && npm run deploy:netlify",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:headless": "cypress run --headless",
    "test:e2e:chrome": "cypress run --browser chrome",
    "test:e2e:firefox": "cypress run --browser firefox",
    "test:smoke": "cypress run --spec 'cypress/e2e/smoke.cy.js'",
    "test:auth": "cypress run --spec 'cypress/e2e/auth.cy.js'",
    "test:projects": "cypress run --spec 'cypress/e2e/projects.cy.js'",
    "test:checklist": "cypress run --spec 'cypress/e2e/checklist.cy.js'",
    "test:ui": "cypress run --spec 'cypress/e2e/ui.cy.js'",
    "cypress:install": "cypress install",
    "cypress:verify": "cypress verify",
    "test:progression": "cypress run --spec 'cypress/e2e/project-progression-sync.cy.js'",
    "test:debug-scores": "cypress run --spec 'cypress/e2e/debug-calculatescores.cy.js'",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "version:show": "node -p \"require('./package.json').version\"",
    "version:auto": "node scripts/auto-version.js",
    "version:auto:feature": "npm version minor",
    "version:auto:hotfix": "npm version patch",
    "version:auto:major": "npm version major"
  },
  "dependencies": {
    "@nuxt/icon": "^1.0.0",
    "@nuxt/image": "^1.0.0",
    "@nuxtjs/i18n": "^10.0.3",
    "@pinia/nuxt": "^0.11.2",
    "firebase": "^12.0.0",
    "nuxt": "^4.0.1",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.4.1",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@iconify-json/fluent": "^1.2.28",
    "@iconify-json/fluent-emoji": "^1.2.3",
    "@iconify-json/heroicons": "^1.2.2",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "cross-env": "^7.0.3",
    "cypress": "^14.5.4",
    "wait-on": "^8.0.4",
    "vite": "^7.0.6",
    "@vitejs/plugin-vue": "^7.0.0"
  }
}
